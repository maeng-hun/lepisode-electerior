model BusinessInfo {
    id String @id @default(cuid(2))
    companyName String
    ceoName String
    address String
    contactNumber String
    email String
    businessNumber String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    deletedAt DateTime?

    imageId String? @unique
    image   File? @relation(fields: [imageId], references: [id])
}